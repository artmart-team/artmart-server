{
  "name": "artmart-server",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "node bin/http.js",
    "db:create": "npx sequelize-cli db:create",
    "db:create:development": "NODE_ENV=development npm run db:create",
    "db:create:test": "NODE_ENV=test npm run db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:development": "NODE_ENV=development npm run db:migrate",
    "db:migrate:test": "NODE_ENV=test npm run db:migrate",
    "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:migrate:undo:all:test": "NODE_ENV=test npm run db:migrate:undo:all",
    "db:migrate:undo:all:development": "NODE_ENV=development npm run db:migrate:undo:all",
    "db:seed:all": "npx sequelize-cli db:seed:all",
    "db:seed:all:test": "NODE_ENV=test npm run db:seed:all",
    "db:seed:all:development": "NODE_ENV=development npm run db:seed:all",
    "dev": "NODE_ENV=development npx nodemon bin/http.js",
    "test": "NODE_ENV=test npx jest --runInBand --detectOpenHandles --force-exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artmart-team/artmart-server.git"
  },
