  "bugs": {
    "url": "https://github.com/artmart-team/artmart-server/issues"
  },
  "homepage": "https://github.com/artmart-team/artmart-server#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "pg": "^8.5.1",
    "sequelize": "^6.5.0"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "jest": "^26.6.3",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.3"
  }
