{
  "name": "artmart-server",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app.js",
    "db:create": "npx sequelize db:create",
    "db:migrate": "npx sequelize db:migrate",
    "db:migrate:undo:all": "npx sequelize db:migrate:undo:all",
    "db:seed:undo:all": "npx sequelize db:seed:undo:all",
    "db:seed:all": "npx sequelize db:seed:all",
    "db-test-start": "set NODE_ENV=test&&npm run db:create&&npm run db:migrate&&npm run db:seed:all",
    "db-dev-start": "set NODE_ENV=development&&npm run db:create&&npm run db:migrate&&npm run db:seed:all",
    "db-test-drop": "set NODE_ENV=test&&npx sequelize db:drop",
    "db-dev-drop": "set NODE_ENV=development&&npx sequelize db:drop",
    "dev": "NODE_ENV=development npx nodemon bin/http.js",
    "test": "set NODE_ENV=test&&npx jest --runInBand --detectOpenHandles --force-exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artmart-team/artmart-server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/artmart-team/artmart-server/issues"
  },
  "homepage": "https://github.com/artmart-team/artmart-server#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "sequelize": "^6.5.0"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.3"
  }
}
